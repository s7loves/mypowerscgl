  <sqlMaps>
    <sqlMap embedded="${Scgl}Empolyees.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}mModule.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}mModulFun.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}mOrg.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}mRole.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}mUser.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}PS_dyxl.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}PS_gt.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}PS_gtsb.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}PS_tq.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}PS_tqbyq.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}PS_tqsb.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}PS_xl.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}rRoleModul.xml,${assembly}"/>
    <sqlMap embedded="${Scgl}rUserRole.xml,${assembly}"/>
<sqlMap embedded="${Scgl}mPost.xml,${assembly}"/>

  </sqlMaps>
</sqlMapConfig>