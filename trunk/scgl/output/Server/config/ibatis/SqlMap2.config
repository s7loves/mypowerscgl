		<sqlMap embedded="${maps}kc_工程类别.xml,${assembly}"/>
		<sqlMap embedded="${maps}kc_工程项目.xml,${assembly}"/>
		<sqlMap embedded="${maps}kc_供货厂家.xml,${assembly}"/>
		<sqlMap embedded="${maps}kc_入库明细表.xml,${assembly}"/>
